{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": {
            "options": "never-prefix"
        },
        "semicolon": {
            "options": "never"
        },
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "no-console": false,
        "ordered-imports": false,
        "arrow-parens": {
            "options": "ban-single-arg-parens"
        }
    },
    "rulesDirectory": []
}